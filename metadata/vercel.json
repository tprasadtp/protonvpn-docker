{
  "github": {
    "silent": true
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/metadata.json"
    },
    {
      "source": "/v1/client/ip",
      "destination": "/api/v1/client/"
    }
  ],
  "redirects": [
    {
      "source": "/v2/client/ip",
      "destination": "https://icanhazip.com/"
    }
  ],
  "functions": {
    "api/v1/client/*.go": {
      "memory": 128,
      "maxDuration": 2
    }
  },
  "headers": [
    {
      "source": "/(404|index)\\.html",
      "headers": [
        {
          "key": "content-type",
          "value": "application/json"
        }
      ]
    },
    {
      "source": "/(metadata|version)\\.json",
      "headers": [
        {
          "key": "content-type",
          "value": "application/json"
        }
      ]
    },
    {
      "source": "/v1/server/(.*)",
      "headers": [
        {
          "key": "content-type",
          "value": "application/json"
        }
      ]
    }
  ]
}
